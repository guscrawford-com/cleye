{
  "name": "@guscrawford.com/cleye",
  "version": "0.0.2-alpha",
  "description": "An unopinionated cli library",
  "main": "index.js",
  "repository": "https://github.com/guscrawford-com/cleye.git",
  "author": "Gus Crawford <crawford.gus@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jasmine": "^3.3.13",
    "cpy-cli": "^2.0.0",
    "jasmine": "^3.4.0",
    "renamer": "^1.1.2",
    "rimraf": "^2.6.3",
    "typedoc": "^0.14.2",
    "typescript": "^3.5.2"
  },
  "scripts": {
    "package":"cpy README.md dist && cpy CHANGELOG.md dist && cpy package.lib.json dist && renamer --find .lib dist/*.json",
    "build": "rimraf dist && rimraf docs && typedoc --out docs ./src && tsc && yarn package",
    "build:test": "tsc -p tsconfig.jasmine.json",
    "test": "yarn build:test && jasmine \"test/**/*[sS]pec.js\""
  }
}
